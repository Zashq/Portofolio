{"ast":null,"code":"import { computed } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useCartStore } from '@/store/cart';\nimport { useToast } from 'vue-toastification';\nexport default {\n  name: 'CartView',\n  setup() {\n    const router = useRouter();\n    const cartStore = useCartStore();\n    const toast = useToast();\n    const items = computed(() => cartStore.items);\n    const itemsCount = computed(() => cartStore.itemsCount);\n    const subtotal = computed(() => cartStore.subtotal);\n    const tax = computed(() => cartStore.tax);\n    const shipping = computed(() => cartStore.shipping);\n    const total = computed(() => cartStore.total);\n    const incrementQuantity = async item => {\n      await cartStore.updateQuantity(item.id, item.quantity + 1);\n    };\n    const decrementQuantity = async item => {\n      if (item.quantity > 1) await cartStore.updateQuantity(item.id, item.quantity - 1);\n    };\n    const removeItem = async item => {\n      await cartStore.removeItem(item.id);\n      toast.success('Item removed');\n    };\n    const clearCart = async () => {\n      await cartStore.clearCart();\n      toast.success('Cart cleared');\n    };\n    return {\n      items,\n      itemsCount,\n      subtotal,\n      tax,\n      shipping,\n      total,\n      incrementQuantity,\n      decrementQuantity,\n      removeItem,\n      clearCart\n    };\n  }\n};","map":{"version":3,"names":["computed","useRouter","useCartStore","useToast","name","setup","router","cartStore","toast","items","itemsCount","subtotal","tax","shipping","total","incrementQuantity","item","updateQuantity","id","quantity","decrementQuantity","removeItem","success","clearCart"],"sources":["C:\\Users\\veszi\\Desktop\\Portofolio\\vue-pwa-webshop\\src\\views\\Cart.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"mb-4\">\n      <v-col cols=\"12\">\n        <h1 class=\"text-h3 font-weight-bold\">Shopping Cart</h1>\n        <p class=\"text-subtitle-1 text-grey\">{{ itemsCount }} items in your cart</p>\n      </v-col>\n    </v-row>\n\n    <v-row v-if=\"items.length === 0\">\n      <v-col cols=\"12\" class=\"text-center py-12\">\n        <v-icon size=\"120\" color=\"grey-lighten-2\">mdi-cart-outline</v-icon>\n        <h2 class=\"text-h4 mt-4 mb-2\">Your cart is empty</h2>\n        <v-btn color=\"primary\" size=\"large\" to=\"/products\">Start Shopping</v-btn>\n      </v-col>\n    </v-row>\n\n    <v-row v-else>\n      <v-col cols=\"12\" md=\"8\">\n        <v-card>\n          <v-list lines=\"three\">\n            <template v-for=\"(item, index) in items\" :key=\"item.id\">\n              <v-list-item>\n                <template v-slot:prepend>\n                  <v-avatar size=\"80\" rounded><v-img :src=\"item.image\"></v-img></v-avatar>\n                </template>\n                <v-list-item-title class=\"font-weight-bold mb-2\">{{ item.title }}</v-list-item-title>\n                <v-list-item-subtitle>\n                  <div class=\"d-flex align-center mt-2\">\n                    <span class=\"text-h6 font-weight-bold primary--text mr-4\">${{ (item.price * item.quantity).toFixed(2) }}</span>\n                  </div>\n                  <div class=\"d-flex align-center mt-3\">\n                    <v-btn icon size=\"small\" variant=\"outlined\" @click=\"decrementQuantity(item)\"><v-icon>mdi-minus</v-icon></v-btn>\n                    <v-text-field :model-value=\"item.quantity\" type=\"number\" hide-details density=\"compact\" class=\"mx-2\" style=\"max-width: 60px;\"></v-text-field>\n                    <v-btn icon size=\"small\" variant=\"outlined\" @click=\"incrementQuantity(item)\"><v-icon>mdi-plus</v-icon></v-btn>\n                    <v-spacer></v-spacer>\n                    <v-btn icon color=\"error\" variant=\"text\" @click=\"removeItem(item)\"><v-icon>mdi-delete</v-icon></v-btn>\n                  </div>\n                </v-list-item-subtitle>\n              </v-list-item>\n              <v-divider v-if=\"index < items.length - 1\"></v-divider>\n            </template>\n          </v-list>\n          <v-card-actions>\n            <v-btn color=\"error\" variant=\"text\" @click=\"clearCart\"><v-icon left>mdi-delete-sweep</v-icon>Clear Cart</v-btn>\n          </v-card-actions>\n        </v-card>\n        <v-btn color=\"secondary\" variant=\"outlined\" size=\"large\" class=\"mt-4\" to=\"/products\"><v-icon left>mdi-arrow-left</v-icon>Continue Shopping</v-btn>\n      </v-col>\n\n      <v-col cols=\"12\" md=\"4\">\n        <v-card>\n          <v-card-title class=\"bg-primary\"><span class=\"white--text\">Order Summary</span></v-card-title>\n          <v-card-text class=\"pa-4\">\n            <v-list>\n              <v-list-item><v-list-item-title>Subtotal</v-list-item-title><template v-slot:append><span class=\"font-weight-bold\">${{ subtotal.toFixed(2) }}</span></template></v-list-item>\n              <v-list-item><v-list-item-title>Tax (8%)</v-list-item-title><template v-slot:append><span class=\"font-weight-bold\">${{ tax.toFixed(2) }}</span></template></v-list-item>\n              <v-list-item><v-list-item-title>Shipping</v-list-item-title><template v-slot:append><span class=\"font-weight-bold\">{{ shipping === 0 ? 'FREE' : `$${shipping.toFixed(2)}` }}</span></template></v-list-item>\n              <v-divider class=\"my-2\"></v-divider>\n              <v-list-item><v-list-item-title class=\"text-h6 font-weight-bold\">Total</v-list-item-title><template v-slot:append><span class=\"text-h6 font-weight-bold primary--text\">${{ total.toFixed(2) }}</span></template></v-list-item>\n            </v-list>\n            <v-btn color=\"primary\" size=\"large\" block class=\"mt-4\" @click=\"$router.push('/checkout')\">Proceed to Checkout<v-icon right>mdi-arrow-right</v-icon></v-btn>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { computed } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { useCartStore } from '@/store/cart'\nimport { useToast } from 'vue-toastification'\n\nexport default {\n  name: 'CartView',\n  setup() {\n    const router = useRouter()\n    const cartStore = useCartStore()\n    const toast = useToast()\n    \n    const items = computed(() => cartStore.items)\n    const itemsCount = computed(() => cartStore.itemsCount)\n    const subtotal = computed(() => cartStore.subtotal)\n    const tax = computed(() => cartStore.tax)\n    const shipping = computed(() => cartStore.shipping)\n    const total = computed(() => cartStore.total)\n    \n    const incrementQuantity = async (item) => { await cartStore.updateQuantity(item.id, item.quantity + 1) }\n    const decrementQuantity = async (item) => { if (item.quantity > 1) await cartStore.updateQuantity(item.id, item.quantity - 1) }\n    const removeItem = async (item) => { await cartStore.removeItem(item.id); toast.success('Item removed') }\n    const clearCart = async () => { await cartStore.clearCart(); toast.success('Cart cleared') }\n    \n    return { items, itemsCount, subtotal, tax, shipping, total, incrementQuantity, decrementQuantity, removeItem, clearCart }\n  }\n}\n</script>\n"],"mappings":"AAsEA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,YAAW,QAAS,cAAa;AAC1C,SAASC,QAAO,QAAS,oBAAmB;AAE5C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,SAAS,CAAC;IACzB,MAAMM,SAAQ,GAAIL,YAAY,CAAC;IAC/B,MAAMM,KAAI,GAAIL,QAAQ,CAAC;IAEvB,MAAMM,KAAI,GAAIT,QAAQ,CAAC,MAAMO,SAAS,CAACE,KAAK;IAC5C,MAAMC,UAAS,GAAIV,QAAQ,CAAC,MAAMO,SAAS,CAACG,UAAU;IACtD,MAAMC,QAAO,GAAIX,QAAQ,CAAC,MAAMO,SAAS,CAACI,QAAQ;IAClD,MAAMC,GAAE,GAAIZ,QAAQ,CAAC,MAAMO,SAAS,CAACK,GAAG;IACxC,MAAMC,QAAO,GAAIb,QAAQ,CAAC,MAAMO,SAAS,CAACM,QAAQ;IAClD,MAAMC,KAAI,GAAId,QAAQ,CAAC,MAAMO,SAAS,CAACO,KAAK;IAE5C,MAAMC,iBAAgB,GAAI,MAAOC,IAAI,IAAK;MAAE,MAAMT,SAAS,CAACU,cAAc,CAACD,IAAI,CAACE,EAAE,EAAEF,IAAI,CAACG,QAAO,GAAI,CAAC;IAAE;IACvG,MAAMC,iBAAgB,GAAI,MAAOJ,IAAI,IAAK;MAAE,IAAIA,IAAI,CAACG,QAAO,GAAI,CAAC,EAAE,MAAMZ,SAAS,