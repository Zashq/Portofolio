{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { useProductsStore } from '@/store/products';\nimport { useCartStore } from '@/store/cart';\nimport { useToast } from 'vue-toastification';\nexport default {\n  name: 'ProductDetailView',\n  setup() {\n    const route = useRoute();\n    const productsStore = useProductsStore();\n    const cartStore = useCartStore();\n    const toast = useToast();\n    const product = ref(null);\n    const loading = ref(true);\n    onMounted(async () => {\n      product.value = await productsStore.getProductById(route.params.id);\n      loading.value = false;\n    });\n    const addToCart = () => {\n      cartStore.addItem(product.value);\n      toast.success('Added to cart!');\n    };\n    return {\n      product,\n      loading,\n      addToCart\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRoute","useProductsStore","useCartStore","useToast","name","setup","route","productsStore","cartStore","toast","product","loading","value","getProductById","params","id","addToCart","addItem","success"],"sources":["C:\\Users\\veszi\\Desktop\\Portofolio\\vue-pwa-webshop\\src\\views\\ProductDetail.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row v-if=\"loading\" class=\"my-12\">\n      <v-col cols=\"12\" class=\"text-center\">\n        <v-progress-circular indeterminate color=\"primary\" size=\"64\"></v-progress-circular>\n        <p class=\"mt-4\">Loading...</p>\n      </v-col>\n    </v-row>\n    <div v-else-if=\"product\">\n      <v-row>\n        <v-col cols=\"12\" md=\"6\">\n          <v-img :src=\"product.image\" max-height=\"500\" contain></v-img>\n        </v-col>\n        <v-col cols=\"12\" md=\"6\">\n          <h1 class=\"text-h4 mb-4\">{{ product.title }}</h1>\n          <p class=\"text-h3 mb-4\">${{ product.price }}</p>\n          <p>{{ product.description }}</p>\n          <v-btn color=\"primary\" size=\"large\" class=\"mt-4\" @click=\"addToCart\">\n            <v-icon left>mdi-cart-plus</v-icon>Add to Cart\n          </v-btn>\n        </v-col>\n      </v-row>\n    </div>\n  </v-container>\n</template>\n\n<script>\nimport { ref, onMounted } from 'vue'\nimport { useRoute } from 'vue-router'\nimport { useProductsStore } from '@/store/products'\nimport { useCartStore } from '@/store/cart'\nimport { useToast } from 'vue-toastification'\n\nexport default {\n  name: 'ProductDetailView',\n  setup() {\n    const route = useRoute()\n    const productsStore = useProductsStore()\n    const cartStore = useCartStore()\n    const toast = useToast()\n    const product = ref(null)\n    const loading = ref(true)\n    \n    onMounted(async () => {\n      product.value = await productsStore.getProductById(route.params.id)\n      loading.value = false\n    })\n    \n    const addToCart = () => {\n      cartStore.addItem(product.value)\n      toast.success('Added to cart!')\n    }\n    \n    return { product, loading, addToCart }\n  }\n}\n</script>\n"],"mappings":"AA2BA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAI;AACnC,SAASC,QAAO,QAAS,YAAW;AACpC,SAASC,gBAAe,QAAS,kBAAiB;AAClD,SAASC,YAAW,QAAS,cAAa;AAC1C,SAASC,QAAO,QAAS,oBAAmB;AAE5C,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;IACvB,MAAMO,aAAY,GAAIN,gBAAgB,CAAC;IACvC,MAAMO,SAAQ,GAAIN,YAAY,CAAC;IAC/B,MAAMO,KAAI,GAAIN,QAAQ,CAAC;IACvB,MAAMO,OAAM,GAAIZ,GAAG,CAAC,IAAI;IACxB,MAAMa,OAAM,GAAIb,GAAG,CAAC,IAAI;IAExBC,SAAS,CAAC,YAAY;MACpBW,OAAO,CAACE,KAAI,GAAI,MAAML,aAAa,CAACM,cAAc,CAACP,KAAK,CAACQ,MAAM,CAACC,EAAE;MAClEJ,OAAO,CAACC,KAAI,GAAI,KAAI;IACtB,CAAC;IAED,MAAMI,SAAQ,GAAIA,CAAA,KAAM;MACtBR,SAAS,CAACS,OAAO,CAACP,OAAO,CAACE,KAAK;MAC/BH,KAAK,CAACS,OAAO,CAAC,gBAAgB;IAChC;IAEA,OAAO;MAAER,OAAO;MAAEC,OAAO;MAAEK;IAAU;EACvC;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}