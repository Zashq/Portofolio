{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport { useProductsStore } from '@/store/products';\nimport ProductCard from '@/components/ProductCard.vue';\nexport default {\n  name: 'CategoriesView',\n  components: {\n    ProductCard\n  },\n  setup() {\n    const productsStore = useProductsStore();\n    const loading = ref(true);\n    const categories = computed(() => productsStore.categories);\n    const products = computed(() => productsStore.products);\n    const featuredProducts = computed(() => productsStore.featuredProducts);\n    const categoriesWithStats = computed(() => {\n      return categories.value.map(category => ({\n        name: category,\n        count: products.value.filter(p => p.category === category).length\n      }));\n    });\n    const getCategoryIcon = category => {\n      const icons = {\n        'electronics': 'mdi-laptop',\n        'jewelery': 'mdi-diamond-stone',\n        \"men's clothing\": 'mdi-tshirt-crew',\n        \"women's clothing\": 'mdi-hanger'\n      };\n      return icons[category] || 'mdi-tag';\n    };\n    onMounted(async () => {\n      await Promise.all([productsStore.loadProducts(), productsStore.loadCategories()]);\n      loading.value = false;\n    });\n    return {\n      loading,\n      categoriesWithStats,\n      featuredProducts,\n      getCategoryIcon\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useProductsStore","ProductCard","name","components","setup","productsStore","loading","categories","products","featuredProducts","categoriesWithStats","value","map","category","count","filter","p","length","getCategoryIcon","icons","Promise","all","loadProducts","loadCategories"],"sources":["C:\\Users\\veszi\\Desktop\\Portofolio\\vue-pwa-webshop\\src\\views\\Categories.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row class=\"mb-6\">\n      <v-col cols=\"12\" class=\"text-center\">\n        <h1 class=\"text-h3 font-weight-bold mb-2\">Shop by Category</h1>\n        <p class=\"text-h6 text-grey\">Find exactly what you're looking for</p>\n      </v-col>\n    </v-row>\n\n    <v-row v-if=\"loading\" class=\"my-12\">\n      <v-col cols=\"12\" class=\"text-center\">\n        <v-progress-circular indeterminate color=\"primary\" size=\"64\"></v-progress-circular>\n      </v-col>\n    </v-row>\n\n    <v-row v-else>\n      <v-col v-for=\"category in categoriesWithStats\" :key=\"category.name\" cols=\"12\" sm=\"6\" md=\"4\">\n        <v-card class=\"category-card\" hover @click=\"$router.push(`/category/${category.name}`)\">\n          <v-card-title class=\"text-h5 font-weight-bold text-capitalize\">{{ category.name }}</v-card-title>\n          <v-card-text>\n            <div class=\"d-flex justify-space-between align-center\">\n              <div>\n                <v-icon left color=\"primary\">{{ getCategoryIcon(category.name) }}</v-icon>\n                <span class=\"font-weight-bold\">{{ category.count }} Products</span>\n              </div>\n            </div>\n          </v-card-text>\n          <v-card-actions>\n            <v-btn color=\"primary\" variant=\"text\" block @click.stop=\"$router.push(`/category/${category.name}`)\">\n              View All Products<v-icon right>mdi-chevron-right</v-icon>\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-col>\n    </v-row>\n\n    <v-row class=\"mt-8\">\n      <v-col cols=\"12\"><h2 class=\"text-h4 mb-4\">Popular This Week</h2></v-col>\n      <v-col v-for=\"product in featuredProducts\" :key=\"product.id\" cols=\"12\" sm=\"6\" md=\"3\">\n        <ProductCard :product=\"product\" />\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { ref, computed, onMounted } from 'vue'\nimport { useProductsStore } from '@/store/products'\nimport ProductCard from '@/components/ProductCard.vue'\n\nexport default {\n  name: 'CategoriesView',\n  components: { ProductCard },\n  setup() {\n    const productsStore = useProductsStore()\n    const loading = ref(true)\n    \n    const categories = computed(() => productsStore.categories)\n    const products = computed(() => productsStore.products)\n    const featuredProducts = computed(() => productsStore.featuredProducts)\n    \n    const categoriesWithStats = computed(() => {\n      return categories.value.map(category => ({\n        name: category,\n        count: products.value.filter(p => p.category === category).length\n      }))\n    })\n    \n    const getCategoryIcon = (category) => {\n      const icons = {\n        'electronics': 'mdi-laptop',\n        'jewelery': 'mdi-diamond-stone',\n        \"men's clothing\": 'mdi-tshirt-crew',\n        \"women's clothing\": 'mdi-hanger'\n      }\n      return icons[category] || 'mdi-tag'\n    }\n    \n    onMounted(async () => {\n      await Promise.all([productsStore.loadProducts(), productsStore.loadCategories()])\n      loading.value = false\n    })\n    \n    return { loading, categoriesWithStats, featuredProducts, getCategoryIcon }\n  }\n}\n</script>\n\n<style scoped>\n.category-card { cursor: pointer; transition: all 0.3s ease; }\n.category-card:hover { transform: translateY(-8px); box-shadow: 0 8px 24px rgba(0,0,0,0.15); }\n</style>\n"],"mappings":"AA8CA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AAC7C,SAASC,gBAAe,QAAS,kBAAiB;AAClD,OAAOC,WAAU,MAAO,8BAA6B;AAErD,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IAAEF;EAAY,CAAC;EAC3BG,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAIL,gBAAgB,CAAC;IACvC,MAAMM,OAAM,GAAIT,GAAG,CAAC,IAAI;IAExB,MAAMU,UAAS,GAAIT,QAAQ,CAAC,MAAMO,aAAa,CAACE,UAAU;IAC1D,MAAMC,QAAO,GAAIV,QAAQ,CAAC,MAAMO,aAAa,CAACG,QAAQ;IACtD,MAAMC,gBAAe,GAAIX,QAAQ,CAAC,MAAMO,aAAa,CAACI,gBAAgB;IAEtE,MAAMC,mBAAkB,GAAIZ,QAAQ,CAAC,MAAM;MACzC,OAAOS,UAAU,CAACI,KAAK,CAACC,GAAG,CAACC,QAAO,KAAM;QACvCX,IAAI,EAAEW,QAAQ;QACdC,KAAK,EAAEN,QAAQ,CAACG,KAAK,CAACI,MAAM,CAACC,CAAA,IAAKA,CAAC,CAACH,QAAO,KAAMA,QAAQ,CAAC,CAACI;MAC7D,CAAC,CAAC;IACJ,CAAC;IAED,MAAMC,eAAc,GAAKL,QAAQ,IAAK;MACpC,MAAMM,KAAI,GAAI;QACZ,aAAa,EAAE,YAAY;QAC3B,