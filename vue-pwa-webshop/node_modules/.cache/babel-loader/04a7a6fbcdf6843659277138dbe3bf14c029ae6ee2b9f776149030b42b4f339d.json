{"ast":null,"code":"import { computed } from 'vue';\nimport { useCartStore } from '@/store/cart';\nexport default {\n  name: 'CartView',\n  setup() {\n    const cartStore = useCartStore();\n    const items = computed(() => cartStore.items);\n    const subtotal = computed(() => cartStore.subtotal);\n    const tax = computed(() => cartStore.tax);\n    const total = computed(() => cartStore.total);\n    const removeItem = id => cartStore.removeItem(id);\n    return {\n      items,\n      subtotal,\n      tax,\n      total,\n      removeItem\n    };\n  }\n};","map":{"version":3,"names":["computed","useCartStore","name","setup","cartStore","items","subtotal","tax","total","removeItem","id"],"sources":["C:\\Users\\veszi\\Desktop\\Portofolio\\vue-pwa-webshop\\src\\views\\Cart.vue"],"sourcesContent":["<template>\n  <v-container>\n    <h1 class=\"text-h3 mb-4\">Shopping Cart</h1>\n    <v-row v-if=\"items.length === 0\">\n      <v-col cols=\"12\" class=\"text-center py-12\">\n        <v-icon size=\"120\" color=\"grey\">mdi-cart-outline</v-icon>\n        <h2 class=\"text-h4 mt-4\">Your cart is empty</h2>\n        <v-btn color=\"primary\" size=\"large\" class=\"mt-4\" to=\"/products\">Start Shopping</v-btn>\n      </v-col>\n    </v-row>\n    <v-row v-else>\n      <v-col cols=\"12\" md=\"8\">\n        <v-card>\n          <v-list>\n            <v-list-item v-for=\"item in items\" :key=\"item.id\">\n              <template v-slot:prepend>\n                <v-avatar size=\"80\"><v-img :src=\"item.image\"></v-img></v-avatar>\n              </template>\n              <v-list-item-title>{{ item.title }}</v-list-item-title>\n              <v-list-item-subtitle>\n                Qty: {{ item.quantity }} x ${{ item.price }}\n              </v-list-item-subtitle>\n              <template v-slot:append>\n                <v-btn icon color=\"error\" @click=\"removeItem(item.id)\">\n                  <v-icon>mdi-delete</v-icon>\n                </v-btn>\n              </template>\n            </v-list-item>\n          </v-list>\n        </v-card>\n      </v-col>\n      <v-col cols=\"12\" md=\"4\">\n        <v-card>\n          <v-card-title>Order Summary</v-card-title>\n          <v-card-text>\n            <div class=\"d-flex justify-space-between mb-2\">\n              <span>Subtotal:</span>\n              <span>${{ subtotal.toFixed(2) }}</span>\n            </div>\n            <div class=\"d-flex justify-space-between mb-2\">\n              <span>Tax:</span>\n              <span>${{ tax.toFixed(2) }}</span>\n            </div>\n            <div class=\"d-flex justify-space-between font-weight-bold\">\n              <span>Total:</span>\n              <span>${{ total.toFixed(2) }}</span>\n            </div>\n            <v-btn color=\"primary\" block class=\"mt-4\" to=\"/checkout\">Proceed to Checkout</v-btn>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { computed } from 'vue'\nimport { useCartStore } from '@/store/cart'\n\nexport default {\n  name: 'CartView',\n  setup() {\n    const cartStore = useCartStore()\n    const items = computed(() => cartStore.items)\n    const subtotal = computed(() => cartStore.subtotal)\n    const tax = computed(() => cartStore.tax)\n    const total = computed(() => cartStore.total)\n    const removeItem = (id) => cartStore.removeItem(id)\n    return { items, subtotal, tax, total, removeItem }\n  }\n}\n</script>\n"],"mappings":"AAwDA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,YAAW,QAAS,cAAa;AAE1C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIH,YAAY,CAAC;IAC/B,MAAMI,KAAI,GAAIL,QAAQ,CAAC,MAAMI,SAAS,CAACC,KAAK;IAC5C,MAAMC,QAAO,GAAIN,QAAQ,CAAC,MAAMI,SAAS,CAACE,QAAQ;IAClD,MAAMC,GAAE,GAAIP,QAAQ,CAAC,MAAMI,SAAS,CAACG,GAAG;IACxC,MAAMC,KAAI,GAAIR,QAAQ,CAAC,MAAMI,SAAS,CAACI,KAAK;IAC5C,MAAMC,UAAS,GAAKC,EAAE,IAAKN,SAAS,CAACK,UAAU,CAACC,EAAE;IAClD,OAAO;MAAEL,KAAK;MAAEC,QAAQ;MAAEC,GAAG;MAAEC,KAAK;MAAEC;IAAW;EACnD;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}