{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref } from 'vue';\nimport { useToast } from 'vue-toastification';\nexport default {\n  name: 'AlertsView',\n  setup() {\n    const toast = useToast();\n    const alerts = ref([{\n      id: '1',\n      productTitle: 'Fjallraven Backpack',\n      productImage: 'https://fakestoreapi.com/img/81fPKd-2AYL._AC_SL1500_.jpg',\n      currentPrice: 109.95,\n      targetPrice: 89.99,\n      createdAt: new Date('2025-01-01').toISOString()\n    }]);\n    const triggeredAlerts = ref([]);\n    const showCreateDialog = ref(false);\n    const deleteAlert = id => {\n      alerts.value = alerts.value.filter(a => a.id !== id);\n      toast.success('Alert deleted');\n    };\n    const formatDate = dateString => {\n      const date = new Date(dateString);\n      return date.toLocaleDateString('en-US', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric'\n      });\n    };\n    return {\n      alerts,\n      triggeredAlerts,\n      showCreateDialog,\n      deleteAlert,\n      formatDate\n    };\n  }\n};","map":{"version":3,"names":["ref","useToast","name","setup","toast","alerts","id","productTitle","productImage","currentPrice","targetPrice","createdAt","Date","toISOString","triggeredAlerts","showCreateDialog","deleteAlert","value","filter","a","success","formatDate","dateString","date","toLocaleDateString","year","month","day"],"sources":["C:\\Users\\veszi\\Desktop\\Portofolio\\vue-pwa-webshop\\src\\views\\Alerts.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row>\n      <v-col cols=\"12\">\n        <h1 class=\"text-h3 font-weight-bold mb-2\">Price Alerts</h1>\n        <p class=\"text-subtitle-1 text-grey\">Get notified when prices drop</p>\n      </v-col>\n    </v-row>\n\n    <!-- Info Card -->\n    <v-row>\n      <v-col cols=\"12\">\n        <v-alert type=\"info\" variant=\"tonal\" class=\"mb-4\">\n          <v-icon left>mdi-information</v-icon>\n          Set up alerts for your favorite products and we'll notify you when prices drop to your target.\n        </v-alert>\n      </v-col>\n    </v-row>\n\n    <!-- Active Alerts -->\n    <v-row>\n      <v-col cols=\"12\">\n        <v-card>\n          <v-card-title class=\"d-flex justify-space-between align-center\">\n            <span>Active Price Alerts</span>\n            <v-btn color=\"primary\" @click=\"showCreateDialog = true\">\n              <v-icon left>mdi-plus</v-icon>\n              New Alert\n            </v-btn>\n          </v-card-title>\n\n          <!-- Empty State -->\n          <v-card-text v-if=\"alerts.length === 0\" class=\"text-center py-12\">\n            <v-icon size=\"120\" color=\"grey-lighten-2\">mdi-bell-off</v-icon>\n            <h3 class=\"text-h5 mt-4 mb-2\">No active alerts</h3>\n            <p class=\"text-grey mb-6\">Create your first price alert to get started</p>\n            <v-btn color=\"primary\" @click=\"showCreateDialog = true\">\n              <v-icon left>mdi-plus</v-icon>\n              Create Alert\n            </v-btn>\n          </v-card-text>\n\n          <!-- Alerts List -->\n          <v-list v-else>\n            <v-list-item\n              v-for=\"alert in alerts\"\n              :key=\"alert.id\"\n            >\n              <template v-slot:prepend>\n                <v-avatar size=\"60\" rounded>\n                  <v-img :src=\"alert.productImage\"></v-img>\n                </v-avatar>\n              </template>\n\n              <v-list-item-title class=\"font-weight-bold\">\n                {{ alert.productTitle }}\n              </v-list-item-title>\n              <v-list-item-subtitle>\n                <div class=\"mt-2\">\n                  <v-chip size=\"small\" color=\"primary\" class=\"mr-2\">\n                    Current: ${{ alert.currentPrice.toFixed(2) }}\n                  </v-chip>\n                  <v-chip size=\"small\" color=\"success\">\n                    Target: ${{ alert.targetPrice.toFixed(2) }}\n                  </v-chip>\n                </div>\n                <p class=\"text-caption mt-1\">\n                  Created {{ formatDate(alert.createdAt) }}\n                </p>\n              </v-list-item-subtitle>\n\n              <template v-slot:append>\n                <v-btn\n                  icon\n                  variant=\"text\"\n                  color=\"error\"\n                  @click=\"deleteAlert(alert.id)\"\n                >\n                  <v-icon>mdi-delete</v-icon>\n                </v-btn>\n              </template>\n            </v-list-item>\n          </v-list>\n        </v-card>\n      </v-col>\n    </v-row>\n\n    <!-- Triggered Alerts History -->\n    <v-row class=\"mt-4\">\n      <v-col cols=\"12\">\n        <v-card>\n          <v-card-title>Recent Notifications</v-card-title>\n          <v-list v-if=\"triggeredAlerts.length > 0\">\n            <v-list-item\n              v-for=\"alert in triggeredAlerts\"\n              :key=\"alert.id\"\n            >\n              <template v-slot:prepend>\n                <v-icon color=\"success\">mdi-bell-check</v-icon>\n              </template>\n              <v-list-item-title>{{ alert.productTitle }}</v-list-item-title>\n              <v-list-item-subtitle>\n                Price dropped from ${{ alert.oldPrice.toFixed(2) }} to \n                ${{ alert.newPrice.toFixed(2) }} ({{ alert.percentDrop.toFixed(1) }}% off)\n              </v-list-item-subtitle>\n            </v-list-item>\n          </v-list>\n          <v-card-text v-else class=\"text-center py-8\">\n            <p class=\"text-grey\">No notifications yet</p>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n\n    <!-- Create Alert Dialog -->\n    <v-dialog v-model=\"showCreateDialog\" max-width=\"500\">\n      <v-card>\n        <v-card-title>Create Price Alert</v-card-title>\n        <v-card-text>\n          <p class=\"text-caption text-grey mb-4\">\n            Browse products and click \"Set Alert\" on any product detail page\n          </p>\n          <v-btn color=\"primary\" block to=\"/products\">\n            Browse Products\n          </v-btn>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn variant=\"text\" @click=\"showCreateDialog = false\">Close</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-container>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport { useToast } from 'vue-toastification'\n\nexport default {\n  name: 'AlertsView',\n  setup() {\n    const toast = useToast()\n    \n    const alerts = ref([\n      {\n        id: '1',\n        productTitle: 'Fjallraven Backpack',\n        productImage: 'https://fakestoreapi.com/img/81fPKd-2AYL._AC_SL1500_.jpg',\n        currentPrice: 109.95,\n        targetPrice: 89.99,\n        createdAt: new Date('2025-01-01').toISOString()\n      }\n    ])\n    \n    const triggeredAlerts = ref([])\n    const showCreateDialog = ref(false)\n    \n    const deleteAlert = (id) => {\n      alerts.value = alerts.value.filter(a => a.id !== id)\n      toast.success('Alert deleted')\n    }\n    \n    const formatDate = (dateString) => {\n      const date = new Date(dateString)\n      return date.toLocaleDateString('en-US', {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric'\n      })\n    }\n    \n    return {\n      alerts,\n      triggeredAlerts,\n      showCreateDialog,\n      deleteAlert,\n      formatDate\n    }\n  }\n}\n</script>\n"],"mappings":";;AAwIA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,oBAAmB;AAE5C,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;IAEvB,MAAMI,MAAK,GAAIL,GAAG,CAAC,CACjB;MACEM,EAAE,EAAE,GAAG;MACPC,YAAY,EAAE,qBAAqB;MACnCC,YAAY,EAAE,0DAA0D;MACxEC,YAAY,EAAE,MAAM;MACpBC,WAAW,EAAE,KAAK;MAClBC,SAAS,EAAE,IAAIC,IAAI,CAAC,YAAY,CAAC,CAACC,WAAW,CAAC;IAChD,EACD;IAED,MAAMC,eAAc,GAAId,GAAG,CAAC,EAAE;IAC9B,MAAMe,g