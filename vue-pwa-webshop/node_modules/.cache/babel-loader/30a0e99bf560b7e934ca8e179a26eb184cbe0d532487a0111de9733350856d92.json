v-select>\n\n            <v-select\n              v-model=\"settings.language\"\n              :items=\"['English', 'Spanish', 'French', 'German']\"\n              label=\"Language\"\n              outlined\n              dense\n            ></v-select>\n          </v-card-text>\n        </v-card>\n\n        <!-- Notification Settings -->\n        <v-card class=\"mb-4\">\n          <v-card-title>Notifications</v-card-title>\n          <v-card-text>\n            <v-switch\n              v-model=\"settings.emailNotifications\"\n              label=\"Email Notifications\"\n              color=\"primary\"\n              hide-details\n              class=\"mb-4\"\n            ></v-switch>\n\n            <v-switch\n              v-model=\"settings.pushNotifications\"\n              label=\"Push Notifications\"\n              color=\"primary\"\n              hide-details\n              class=\"mb-4\"\n            ></v-switch>\n\n            <v-switch\n              v-model=\"settings.orderUpdates\"\n              label=\"Order Status Updates\"\n              color=\"primary\"\n              hide-details\n              class=\"mb-4\"\n            ></v-switch>\n\n            <v-switch\n              v-model=\"settings.priceAlerts\"\n              label=\"Price Drop Alerts\"\n              color=\"primary\"\n              hide-details\n              class=\"mb-4\"\n            ></v-switch>\n\n            <v-switch\n              v-model=\"settings.newsletter\"\n              label=\"Newsletter & Promotions\"\n              color=\"primary\"\n              hide-details\n            ></v-switch>\n          </v-card-text>\n        </v-card>\n\n        <!-- Privacy Settings -->\n        <v-card class=\"mb-4\">\n          <v-card-title>Privacy</v-card-title>\n          <v-card-text>\n            <v-switch\n              v-model=\"settings.personalizedAds\"\n              label=\"Personalized Recommendations\"\n              color=\"primary\"\n              hide-details\n              class=\"mb-4\"\n            ></v-switch>\n\n            <v-switch\n              v-model=\"settings.dataCollection\"\n              label=\"Usage Data Collection\"\n              color=\"primary\"\n              hide-details\n            ></v-switch>\n\n            <v-divider class=\"my-4\"></v-divider>\n\n            <v-btn variant=\"text\" color=\"error\">\n              <v-icon left>mdi-download</v-icon>\n              Download My Data\n            </v-btn>\n          </v-card-text>\n        </v-card>\n\n        <!-- Account Management -->\n        <v-card class=\"mb-4\">\n          <v-card-title>Account Management</v-card-title>\n          <v-card-text>\n            <v-btn color=\"error\" variant=\"outlined\" block class=\"mb-2\">\n              <v-icon left>mdi-delete-forever</v-icon>\n              Delete Account\n            </v-btn>\n            <p class=\"text-caption text-grey text-center\">\n              This action cannot be undone\n            </p>\n          </v-card-text>\n        </v-card>\n\n        <!-- Save Button -->\n        <v-btn\n          color=\"primary\"\n          size=\"large\"\n          :loading=\"saving\"\n          @click=\"saveSettings\"\n        >\n          <v-icon left>mdi-content-save</v-icon>\n          Save Settings\n        </v-btn>\n      </v-col>\n\n      <!-- Info Sidebar -->\n      <v-col cols=\"12\" md=\"4\">\n        <v-card class=\"mb-4\">\n          <v-card-title>App Information</v-card-title>\n          <v-card-text>\n            <v-list density=\"compact\">\n              <v-list-item>\n                <v-list-item-title>Version</v-list-item-title>\n                <v-list-item-subtitle>1.0.0</v-list-item-subtitle>\n              </v-list-item>\n              <v-list-item>\n                <v-list-item-title>Build</v-list-item-title>\n                <v-list-item-subtitle>2025.01.01</v-list-item-subtitle>\n              </v-list-item>\n            </v-list>\n          </v-card-text>\n        </v-card>\n\n        <v-card>\n          <v-card-title>Support</v-card-title>\n          <v-card-text>\n            <v-btn variant=\"outlined\" block class=\"mb-2\">\n              <v-icon left>mdi-help-circle</v-icon>\n              Help Center\n            </v-btn>\n            <v-btn variant=\"outlined\" block class=\"mb-2\">\n              <v-icon left>mdi-file-document</v-icon>\n              Terms of Service\n            </v-btn>\n            <v-btn variant=\"outlined\" block>\n              <v-icon left>mdi-shield-check</v-icon>\n              Privacy Policy\n            </v-btn>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport { useToast } from 'vue-toastification'\n\nexport default {\n  name: 'SettingsView',\n  setup() {\n    const toast = useToast()\n    \n    const settings = ref({\n      theme: 'light',\n      language: 'English',\n      emailNotifications: true,\n      pushNotifications: true,\n      orderUpdates: true,\n      priceAlerts: true,\n      newsletter: false,\n      personalizedAds: true,\n      dataCollection: true\n    })\n    \n    const saving = ref(false)\n    \n    const saveSettings = async () => {\n      saving.value = true\n      try {\n        await new Promise(resolve => setTimeout(resolve, 1000))\n        toast.success('Settings saved successfully')\n      } catch (error) {\n        toast.error('Failed to save settings')\n      } finally {\n        saving.value = false\n      }\n    }\n    \n    return {\n      settings,\n      saving,\n      saveSettings\n    }\n  }\n}\n</script>\n"],"mappings":"AA6KA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,oBAAmB;AAE5C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC;IAEvB,MAAMI,QAAO,GAAIL,GAAG,CAAC;MACnBM,KAAK,EAAE,OAAO;MACdC,QAAQ,EAAE,SAAS;MACnBC,kBAAkB,EAAE,IAAI;MACxBC,iBAAiB,EAAE,IAAI;MACvBC,YAAY,EAAE,IAAI;MAClBC,WAAW,EAAE,IAAI;MACjBC,UAAU,EAAE,KAAK;MACjBC,eAAe,EAAE,IAAI;M