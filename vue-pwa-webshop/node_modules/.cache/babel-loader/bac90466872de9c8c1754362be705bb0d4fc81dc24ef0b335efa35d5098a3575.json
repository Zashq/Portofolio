{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport { useProductsStore } from '@/store/products';\nimport { useToast } from 'vue-toastification';\nimport ProductCard from '@/components/ProductCard.vue';\nexport default {\n  name: 'HomeView',\n  components: {\n    ProductCard\n  },\n  setup() {\n    const productsStore = useProductsStore();\n    const toast = useToast();\n    const email = ref('');\n    const slides = ref([{\n      src: 'https://images.unsplash.com/photo-1472851294608-062f824d29cc',\n      title: 'Welcome to VueShop',\n      subtitle: 'Your one-stop shop for everything'\n    }, {\n      src: 'https://images.unsplash.com/photo-1556742049-0cfed4f6a45d',\n      title: 'New Arrivals',\n      subtitle: 'Check out our latest products'\n    }, {\n      src: 'https://images.unsplash.com/photo-1607082348824-0a96f2a4b9da',\n      title: 'Special Offers',\n      subtitle: 'Save big on selected items'\n    }]);\n    const features = ref([{\n      icon: 'mdi-truck-delivery',\n      title: 'Free Shipping',\n      description: 'Free delivery on orders over $50'\n    }, {\n      icon: 'mdi-shield-check',\n      title: 'Secure Payment',\n      description: 'Your payment information is safe with us'\n    }, {\n      icon: 'mdi-replay',\n      title: '30-Day Returns',\n      description: 'Not satisfied? Return within 30 days'\n    }]);\n    const categories = computed(() => productsStore.categories);\n    const featuredProducts = computed(() => productsStore.featuredProducts);\n    const getCategoryIcon = category => {\n      const icons = {\n        'electronics': 'mdi-laptop',\n        'jewelery': 'mdi-diamond-stone',\n        \"men's clothing\": 'mdi-tshirt-crew',\n        \"women's clothing\": 'mdi-hanger'\n      };\n      return icons[category] || 'mdi-tag';\n    };\n    const subscribe = () => {\n      if (!email.value) {\n        toast.error('Please enter your email');\n        return;\n      }\n      if (!email.value.match(/^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/)) {\n        toast.error('Please enter a valid email');\n        return;\n      }\n\n      // Here you would normally save to Firebase\n      toast.success('Successfully subscribed to newsletter!');\n      email.value = '';\n    };\n    onMounted(async () => {\n      await Promise.all([productsStore.loadProducts(), productsStore.loadCategories()]);\n    });\n    return {\n      email,\n      slides,\n      features,\n      categories,\n      featuredProducts,\n      getCategoryIcon,\n      subscribe\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useProductsStore","useToast","ProductCard","name","components","setup","productsStore","toast","email","slides","src","title","subtitle","features","icon","description","categories","featuredProducts","getCategoryIcon","category","icons","subscribe","value","error","match","success","Promise","all","loadProducts","loadCategories"],"sources":["C:\\Users\\veszi\\Desktop\\Portofolio\\vue-pwa-webshop\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <!-- Hero Section -->\r\n    <v-row class=\"mb-8\">\r\n      <v-col cols=\"12\">\r\n        <v-carousel\r\n          cycle\r\n          height=\"400\"\r\n          hide-delimiters\r\n          show-arrows=\"hover\"\r\n        >\r\n          <v-carousel-item\r\n            v-for=\"(slide, i) in slides\"\r\n            :key=\"i\"\r\n            :src=\"slide.src\"\r\n            cover\r\n          >\r\n            <div class=\"d-flex fill-height justify-center align-center\">\r\n              <div class=\"text-center\">\r\n                <h1 class=\"text-h2 font-weight-bold white--text mb-4\">\r\n                  {{ slide.title }}\r\n                </h1>\r\n                <p class=\"text-h5 white--text\">\r\n                  {{ slide.subtitle }}\r\n                </p>\r\n                <v-btn\r\n                  class=\"mt-4\"\r\n                  color=\"primary\"\r\n                  size=\"large\"\r\n                  @click=\"$router.push('/products')\"\r\n                >\r\n                  Shop Now\r\n                </v-btn>\r\n              </div>\r\n            </div>\r\n          </v-carousel-item>\r\n        </v-carousel>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Featured Categories -->\r\n    <v-row class=\"mb-8\">\r\n      <v-col cols=\"12\">\r\n        <h2 class=\"text-h4 mb-4\">Shop by Category</h2>\r\n      </v-col>\r\n      <v-col\r\n        v-for=\"category in categories\"\r\n        :key=\"category\"\r\n        cols=\"12\"\r\n        sm=\"6\"\r\n        md=\"3\"\r\n      >\r\n        <v-card\r\n          class=\"category-card\"\r\n          @click=\"$router.push(`/category/${category}`)\"\r\n        >\r\n          <v-card-text class=\"text-center py-8\">\r\n            <v-icon size=\"48\" color=\"primary\">\r\n              {{ getCategoryIcon(category) }}\r\n            </v-icon>\r\n            <h3 class=\"text-h6 mt-4 text-capitalize\">\r\n              {{ category }}\r\n            </h3>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Featured Products -->\r\n    <v-row class=\"mb-8\">\r\n      <v-col cols=\"12\">\r\n        <h2 class=\"text-h4 mb-4\">Featured Products</h2>\r\n      </v-col>\r\n      <v-col\r\n        v-for=\"product in featuredProducts\"\r\n        :key=\"product.id\"\r\n        cols=\"12\"\r\n        sm=\"6\"\r\n        md=\"3\"\r\n      >\r\n        <ProductCard :product=\"product\" />\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Special Offers Banner -->\r\n    <v-row class=\"mb-8\">\r\n      <v-col cols=\"12\">\r\n        <v-card color=\"primary\" dark>\r\n          <v-card-text class=\"text-center py-8\">\r\n            <v-icon size=\"48\">mdi-sale</v-icon>\r\n            <h2 class=\"text-h4 mt-4\">Special Offers</h2>\r\n            <p class=\"text-h6 mt-2\">\r\n              Get up to 50% off on selected items!\r\n            </p>\r\n            <v-btn\r\n              class=\"mt-4\"\r\n              color=\"white\"\r\n              variant=\"outlined\"\r\n              @click=\"$router.push('/products?sale=true')\"\r\n            >\r\n              View Deals\r\n            </v-btn>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Features Section -->\r\n    <v-row>\r\n      <v-col cols=\"12\">\r\n        <h2 class=\"text-h4 mb-4\">Why Shop With Us</h2>\r\n      </v-col>\r\n      <v-col\r\n        v-for=\"feature in features\"\r\n        :key=\"feature.title\"\r\n        cols=\"12\"\r\n        md=\"4\"\r\n      >\r\n        <v-card flat>\r\n          <v-card-text class=\"text-center\">\r\n            <v-icon size=\"48\" color=\"primary\">\r\n              {{ feature.icon }}\r\n            </v-icon>\r\n            <h3 class=\"text-h6 mt-4\">{{ feature.title }}</h3>\r\n            <p class=\"mt-2\">{{ feature.description }}</p>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Newsletter Subscription -->\r\n    <v-row class=\"mt-8\">\r\n      <v-col cols=\"12\">\r\n        <v-card>\r\n          <v-card-text class=\"text-center py-8\">\r\n            <v-icon size=\"48\" color=\"primary\">mdi-email-newsletter</v-icon>\r\n            <h2 class=\"text-h4 mt-4\">Stay Updated</h2>\r\n            <p class=\"mt-2\">Subscribe to our newsletter for exclusive offers and updates</p>\r\n            <v-row justify=\"center\" class=\"mt-4\">\r\n              <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                <v-text-field\r\n                  v-model=\"email\"\r\n                  label=\"Enter your email\"\r\n                  outlined\r\n                  append-inner-icon=\"mdi-send\"\r\n                  @click:append-inner=\"subscribe\"\r\n                  @keyup.enter=\"subscribe\"\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from 'vue'\r\nimport { useProductsStore } from '@/store/products'\r\nimport { useToast } from 'vue-toastification'\r\nimport ProductCard from '@/components/ProductCard.vue'\r\n\r\nexport default {\r\n  name: 'HomeView',\r\n  components: {\r\n    ProductCard\r\n  },\r\n  setup() {\r\n    const productsStore = useProductsStore()\r\n    const toast = useToast()\r\n    \r\n    const email = ref('')\r\n    \r\n    const slides = ref([\r\n      {\r\n        src: 'https://images.unsplash.com/photo-1472851294608-062f824d29cc',\r\n        title: 'Welcome to VueShop',\r\n        subtitle: 'Your one-stop shop for everything'\r\n      },\r\n      {\r\n        src: 'https://images.unsplash.com/photo-1556742049-0cfed4f6a45d',\r\n        title: 'New Arrivals',\r\n        subtitle: 'Check out our latest products'\r\n      },\r\n      {\r\n        src: 'https://images.unsplash.com/photo-1607082348824-0a96f2a4b9da',\r\n        title: 'Special Offers',\r\n        subtitle: 'Save big on selected items'\r\n      }\r\n    ])\r\n    \r\n    const features = ref([\r\n      {\r\n        icon: 'mdi-truck-delivery',\r\n        title: 'Free Shipping',\r\n        description: 'Free delivery on orders over $50'\r\n      },\r\n      {\r\n        icon: 'mdi-shield-check',\r\n        title: 'Secure Payment',\r\n        description: 'Your payment information is safe with us'\r\n      },\r\n      {\r\n        icon: 'mdi-replay',\r\n        title: '30-Day Returns',\r\n        description: 'Not satisfied? Return within 30 days'\r\n      }\r\n    ])\r\n    \r\n    const categories = computed(() => productsStore.categories)\r\n    const featuredProducts = computed(() => productsStore.featuredProducts)\r\n    \r\n    const getCategoryIcon = (category) => {\r\n      const icons = {\r\n        'electronics': 'mdi-laptop',\r\n        'jewelery': 'mdi-diamond-stone',\r\n        \"men's clothing\": 'mdi-tshirt-crew',\r\n        \"women's clothing\": 'mdi-hanger'\r\n      }\r\n      return icons[category] || 'mdi-tag'\r\n    }\r\n    \r\n    const subscribe = () => {\r\n      if (!email.value) {\r\n        toast.error('Please enter your email')\r\n        return\r\n      }\r\n      \r\n      if (!email.value.match(/^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/)) {\r\n        toast.error('Please enter a valid email')\r\n        return\r\n      }\r\n      \r\n      // Here you would normally save to Firebase\r\n      toast.success('Successfully subscribed to newsletter!')\r\n      email.value = ''\r\n    }\r\n    \r\n    onMounted(async () => {\r\n      await Promise.all([\r\n        productsStore.loadProducts(),\r\n        productsStore.loadCategories()\r\n      ])\r\n    })\r\n    \r\n    return {\r\n      email,\r\n      slides,\r\n      features,\r\n      categories,\r\n      featuredProducts,\r\n      getCategoryIcon,\r\n      subscribe\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.category-card {\r\n  cursor: pointer;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.category-card:hover {\r\n  transform: translateY(-4px);\r\n  box-shadow: 0 4px 20px rgba(0,0,0,0.1);\r\n}\r\n</style>\r\n"],"mappings":"AA8JA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAI;AAC7C,SAASC,gBAAe,QAAS,kBAAiB;AAClD,SAASC,QAAO,QAAS,oBAAmB;AAC5C,OAAOC,WAAU,MAAO,8BAA6B;AAErD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,aAAY,GAAIN,gBAAgB,CAAC;IACvC,MAAMO,KAAI,GAAIN,QAAQ,CAAC;IAEvB,MAAMO,KAAI,GAAIX,GAAG,CAAC,EAAE;IAEpB,MAAMY,MAAK,GAAIZ,GAAG,CAAC,CACjB;MACEa,GAAG,EAAE,8DAA8D;MACnEC,KAAK,EAAE,oBAAoB;MAC3BC,QAAQ,EAAE;IACZ,CAAC,EACD;MACEF,GAAG,EAAE,2DAA2D;MAChEC,KAAK,EAAE,cAAc;MACrBC,QAAQ,EAAE;IACZ,CAAC,EACD;MACEF,GAAG,EAAE,8DAA8D;MACnEC,KAAK,EAAE,gBAAgB;MACvBC,QAAQ,EAAE;IACZ,EACD;IAED,MAAMC,QAAO,GAAIhB,GAAG,CAAC,CACnB;MACEiB,IAAI,EAAE,oBAAoB;MAC1BH,KAAK,EAAE,eAAe;MACtBI,WAAW,EAAE;IACf,CAAC,EACD;MACED,IAAI,EAAE,kBAAkB;MACxBH,KAAK,EAAE,gBAAgB;MACvBI,W